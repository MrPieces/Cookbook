{
  "gitbook": "^3.2.2",
  "title": "ArangoDB Cookbook",
  "author": "ArangoDB GmbH",
  "description": "Recipes for ArangoDB - the native multi-model NoSQL database",
  "language": "en",
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "toggle-chapters",
    "addcssjs",
    "anchorjs",
    "sitemap-general",
    "ga",
    "edit-link",
    "canonical-link",
    "localized-footer"
  ],
  "pdf": {
    "fontSize": 12,
    "toc": true,
    "margin": {
      "right": 60,
      "left": 60,
      "top": 35,
      "bottom": 35
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "pluginsConfig": {
    "addcssjs": {
      "js": ["styles/header.js"],
      "css": ["styles/header.css"]
    },
    "sitemap-general": {
      "prefix": "https://docs.arangodb.com/3.1/cookbook/"
    },
    "ga": {
      "token": "UA-81053435-2"
    },
    "edit-link": {
      "base": "https://github.com/arangodb/Cookbook/edit/master/recipes",
      "label": "Edit Page"
    },
    "canonical-link": {
      "baseURL": "https://docs.arangodb.com/3.1/cookbook"
    },
    "localized-footer": {
      "filename": "FOOTER.html"
    }
  }
}
